{% extends 'base.html.twig' %}

{% block title %}Nota{% endblock %}

{% block body %}
    <h1>Nota</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ notum.id }}</td>
            </tr>
            <tr>
                <th>Calificacion</th>
                <td>{{ notum.calificacion }}</td>
            </tr>
            <tr>
                <th>FechaRegistro</th>
                <td>{{ notum.fechaRegistro ? notum.fechaRegistro|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Aprobado</th>
                <td>{{ notum.aprobado ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_nota_index') }}">back to list</a>

    <a href="{{ path('app_nota_edit', {'id': notum.id}) }}">edit</a>

    {{ include('nota/_delete_form.html.twig') }}
{% endblock %}
